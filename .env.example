# Sharp Sports Predictor - Environment Variables
# Copy this to .env and fill in your values

# College Football Data API (required)
# Get your free key at: https://collegefootballdata.com/key
CFBD_API_KEY=your_cfbd_api_key_here

# The Odds API (optional - for live odds)
THE_ODDS_API_KEY=your_odds_api_key_here

# Debug mode (optional)
DEBUG=false
LOG_LEVEL=INFO
